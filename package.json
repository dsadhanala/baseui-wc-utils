{
  "name": "baseui-wc-utils",
  "version": "0.1.0",
  "description": "Web component utilities to reuse common code",
  "author": "Durga Prasad Sadhanala <dsadhanala@gmail.com>",
  "license": "MIT",
  "main": "dist/baseui-wc-utils.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
    "lint": "./node_modules/.bin/eslint src/**/*.js --ignore-pattern 'dist/**/*' --ignore-pattern 'docs/**/*'",
    "webpackServer": "./node_modules/.bin/webpack-dev-server --config webpack.config.js",
    "start": "npm install && npm run dev",
    "dev": "npm run lint && npm run webpackServer",
    "wpBuild": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.js",
    "rollupBuild": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/rollup -c",
    "release": "npm run lint && npm run rollupBuild && npm run docs",
    "bundleServ": "./node_modules/.bin/live-server --port=1919 --host=0.0.0.0 --open='./demo/'"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jsdoc": "^3.4.2",
    "minami": "^1.1.1",
    "open-browser-webpack-plugin": "^0.0.5",
    "rollup": "^0.53.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "webpack": "^3.10.0",
    "webpack-dashboard": "0.4.0",
    "webpack-dev-server": "^2.10.1"
  },
  "dependencies": {
    "live-server": "^1.2.0"
  }
}
