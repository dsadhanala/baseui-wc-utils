{
  "name": "baseui-wc-utils",
  "version": "0.0.3",
  "description": "Web component utilities to reuse common code",
  "author": "Durga Prasad Sadhanala <dsadhanala@gmail.com>",
  "license": "MIT",
  "main": "dist/baseui-wc-utils.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
    "lint": "./node_modules/.bin/eslint src/**/*.js --ignore-pattern 'dist/**/*' --ignore-pattern 'docs/**/*'",
    "webpackServer": "./node_modules/.bin/webpack-dev-server --config webpack.config.js",
    "start": "npm install && npm run dev",
    "dev": "npm run lint && npm run webpackServer",
    "prod": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.js",
    "release": "npm run lint && npm run prod && npm run docs"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "jsdoc": "^3.4.2",
    "minami": "^1.1.1",
    "open-browser-webpack-plugin": "^0.0.3",
    "webpack": "beta",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "beta"
  }
}
